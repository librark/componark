{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "lib",
    "paths": {
      "@base*": [
          "base"
          "base*"
      ],
      "*": [
        "*"
      ]
    }
  }
}
